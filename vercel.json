{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "server.js"
        }
    ],
    "crons": [
        {
            "path": "/api/scrap-all-new-jobs",
            "schedule": "43 10 * * *"
        },
        {
            "path": "/api/delete-old-jobs",
            "schedule": "45 10 * * *"
        }
    ]
}